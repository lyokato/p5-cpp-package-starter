#!/usr/bin/env perl

eval 'exec /usr/bin/perl -w -S $0 ${1+"$@"}' if 0;

use strict;
use warnings;

our $VERSION = '0.0.6';

use Getopt::Long;
use Pod::Usage;

my $help = @ARGV == 0;
my ($project, $author, $license);
my %opt = (
    'help|?'      => \$help,
    'project|p=s' => \$project,
    'author|a=s'  => \$author,
    'license|l=s' => \$license,
);

GetOptions(%opt);

pod2usage(1) if $help;
pod2usage(1) unless $project;
pod2usage(1) unless $author;

$license ||= q{BSD};

my $starter = CPP::Package::Starter->new(
    project_name => $project,
    author       => $author,
    license      => $license,
);

$starter->gen_package();

package CPP::Package::Starter;

=head1 NAME

cpppackage - This program allows you to build C++ package template easily

=head1 SYNOPSIS

    cpppackage [options]

Example:

    cpppackage --project MyProj --author example@example.org --license BSD

Options:

    -project  project name 
    -author   author name
    -license  license type (now it supports MIT|APACHE2|BSD)

=head1 DESCRIPTION

Generate C++ package template.
It is based on CMake and googletest.

=head1 AUTHOR

Lyo Kato, E<lt>lyo.kato@gmail.comE<gt>

=head1 COPYRIGHT AND LICENSE

Copyright 2010 Lyo Kato. All rights reserved.

This library is free software. You can redistribute it and/or modify it under
the same terms as perl itself.

=cut

use File::Spec ();
use IO::File ();

use constant LICENSE_MIT     => q{MIT};
use constant LICENSE_APACHE2 => q{Apache2};
use constant LICENSE_BSD     => q{BSD};

our $VERSION = '0.01';

sub new {
    my ($class, %args) = @_;
    my $self = bless {
        project_name => "MyLib",
        author       => q{<author@example.org>},
        license      => LICENSE_MIT,
        %args, 
    }, $class;
    return $self;
}

sub _mkdir {
    my ($self, $path) = @_;
    unless (-e $path && -d _) {
        mkdir($path, 0755)
            or die "Coundn't mkdir $path.";
    }
    print "Created directory: ";
    print $path, "\n";
}

sub _mkfile {
    my ($self, $path, $content) = @_;
    unless (-e $path && -f _) {
        my $fh = IO::File->new($path, "w")
            or die "Couldn't write file $path";
        $fh->print($$content);
        $fh->close();
    }
    print "Created file     : ";
    print $path, "\n";
}

sub get_project_name {
    my ($self) = @_;
    return $self->{project_name};
}

sub get_package_name {
    my ($self) = @_;
    return lc $self->{project_name};
}

sub gen_package {
    my ($self) = @_;
    $self->gen_project_dir();
    $self->gen_cmake_file();
    $self->gen_readme_file();
    $self->gen_gitignore_file();
    $self->gen_license_file();
    $self->gen_changelog_file();
    $self->gen_build_dir();
    $self->gen_include_dir();
    $self->gen_src_dir();
    $self->gen_gtest_dir();
    $self->gen_tests_dir();
    $self->gen_scripts_dir();
    $self->gen_examples_dir();
    $self->print_welcome_message();
}

sub print_welcome_message {
    my $self = shift;
    my $template = <<EOF;


================================================================
  ____ ____  ____       ____            _                    
 / ___|  _ \|  _ \     |  _ \ __ _  ___| | ____ _  __ _  ___ 
| |   | |_) | |_) |____| |_) / _` |/ __| |/ / _` |/ _` |/ _ \
| |___|  __/|  __/_____|  __/ (_| | (__|   < (_| | (_| |  __/
 \____|_|   |_|        |_|   \__,_|\___|_|\_\__,_|\__, |\___|
             ____  _             _            
            / ___|| |_ __ _ _ __| |_ ___ _ __ 
            \___ \| __/ _` | '__| __/ _ \ '__|
             ___) | || (_| | |  | ||  __/ |   
            |____/ \__\__,_|_|   \__\___|_|   
================================================================

Completed to build your project template.
Now then, move to your project directory,
and try to make an new class!

  cd ./%s
  ./scripts/genclass --name MyClass1
  ./scripts/genclass --name MyClass2

  ./scripts/test
  (./scripts/test --verbose)

  ./scripts/clean

  ./scripts/install


EOF
    printf($template, $self->get_project_name());
}

sub gen_project_dir {
    my ($self) = @_;
    my $path = File::Spec->catdir(File::Spec->curdir,
        $self->get_project_name());
    $self->_mkdir($path);
}

sub gen_cmake_file {
    my ($self) = @_;
    my $path = File::Spec->catfile(File::Spec->curdir,
        $self->get_project_name(), q{CMakeLists.txt});
    my $template = <<'EOF';
CMAKE_MINIMUM_REQUIRED(VERSION 2.8)
ENABLE_TESTING()

PROJECT(%s)
SET(%s_VERSION_MAJOR 1)
SET(%s_VERSION_MINOR 0)

# FIND_PACKAGE(Boost REQUIRED)
## FIND_PACKAGE(Boost 1.45 COMPONENTS filesystem regex REQUIRED)
# LINK_DIRECTORIES(${Boost_LIBRARY_DIRS})
# INCLUDE_DIRECTORIES(${Boost_INCLUDE_DIRS})

SET(CMAKE_CXS_FLAGS "-Wall")
SET(CMAKE_CXX_FLAGS_DEBUG "-g")
SET(CMAKE_CXX_FLAGS_RELEASE "-O2")
# SET(CMAKE_C_FLAGS "-Wall")
# SET(CMAKE_C_FLAGS_DEBUG "-g")
# SET(CMAKE_C_FLAGS_RELEASE "-O2")

ADD_SUBDIRECTORY(src)
ADD_SUBDIRECTORY(gtest)
ADD_SUBDIRECTORY(tests)
ADD_SUBDIRECTORY(examples)
EOF
    my $proj = $self->get_project_name();
    my $file = sprintf($template, $proj, $proj, $proj);
    $self->_mkfile($path, \$file);
}

sub gen_license_file {
    my ($self) = @_;
    my $path = File::Spec->catfile(File::Spec->curdir,
        $self->get_project_name(), q{LICENSE});
    my $file = CPP::Package::Starter::License::Factory->gen_license($self->{license}, $self->{author});
    $self->_mkfile($path, \$file);
}

sub gen_gitignore_file {
    my ($self) = @_;
    my $path = File::Spec->catfile(File::Spec->curdir,
        $self->get_project_name(), q{.gitignore});
    my $file = <<EOF;
build/*
build/**/*
EOF
    $self->_mkfile($path, \$file);
}

sub gen_readme_file {
    my ($self) = @_;
    my $path = File::Spec->catfile(File::Spec->curdir,
        $self->get_project_name(), q{README});
    my $file = <<EOF;
=======================================================================
 INSTALL
=======================================================================

1. cd build
2. cmake .. -DBUILD_SHARED_LIBS=ON -DCMAKE_BUILD_TYPE=Release

parameters
    - BUILD_SHARED_LIBS (ON|OFF)
    - CMAKE_BUILD_TYPE (Debug|Release)
    - CMAKE_INSTALL_PREFIX (/usr/local)

3. make
4. make test
5. make install

EOF
    $self->_mkfile($path, \$file);
}

sub gen_changelog_file {
    my ($self) = @_;
    my $path = File::Spec->catfile(File::Spec->curdir,
        $self->get_project_name(), q{Changes});
    my $template = <<EOF;
Revision history for %s

1.0.0 %s
    - build
EOF
    my $localtime = localtime();
    my $file = sprintf($template,
            $self->get_project_name(),
            $localtime);
    $self->_mkfile($path, \$file);
}

sub gen_build_dir {
    my ($self) = @_;
    my $path = File::Spec->catdir(File::Spec->curdir,
        $self->get_project_name(), q{build});
    $self->_mkdir($path);
}

sub gen_include_dir {
    my ($self) = @_;
    my $path = File::Spec->catdir(File::Spec->curdir,
        $self->get_project_name(), q{include});
    $self->_mkdir($path);

    $path = File::Spec->catdir($path,
        $self->get_package_name());
    $self->_mkdir($path);
}

sub gen_src_dir {
    my ($self) = @_;
    my $path = File::Spec->catdir(File::Spec->curdir,
        $self->get_project_name(), q{src});
    $self->_mkdir($path);
    my $cmake_file = File::Spec->catfile($path, q{CMakeLists.txt});
    my $template = <<'EOF';
INCLUDE_DIRECTORIES(${%s_SOURCE_DIR}/include)

FILE(GLOB src_files ${%s_SOURCE_DIR}/src/*)
ADD_LIBRARY(%s SHARED ${src_files})
ADD_LIBRARY(%s_static STATIC ${src_files})
# SET_TARGET_PROPERTIES(%s_static PROPERTIES OUTPUT_NAME %s)

FILE(GLOB header_files ${%s_SOURCE_DIR}/include/%s/*)
INSTALL(FILES ${header_files} DESTINATION include/%s)
INSTALL(TARGETS %s DESTINATION lib)
EOF
    my $proj    = $self->get_project_name();
    my $package = $self->get_package_name();
    my $file = sprintf($template, $proj, $proj,
            $package, $package, $package, $package, $proj, $package, $package, $package);
    $self->_mkfile($cmake_file, \$file);
}

sub gen_gtest_dir {
    my ($self) = @_;
    my $path = File::Spec->catdir(File::Spec->curdir,
        $self->get_project_name(), q{gtest});
    $self->_mkdir($path);
    my $cmake_file = File::Spec->catfile($path, q{CMakeLists.txt});
    my $template = <<'EOF';
INCLUDE_DIRECTORIES(${%s_SOURCE_DIR}/gtest/include)
ADD_LIBRARY(gtest STATIC src/gtest-all.cc)
ADD_LIBRARY(gtest-main STATIC src/gtest_main.cc)
EOF
    my $proj = $self->get_project_name();
    my $file = sprintf($template, $proj);
    $self->_mkfile($cmake_file, \$file);

    my $include_dir = File::Spec->catdir($path, q{include});
    $self->_mkdir($include_dir);

    my $gtest_dir = File::Spec->catdir($include_dir, q{gtest});
    $self->_mkdir($gtest_dir);

    my $internal_dir = File::Spec->catdir($gtest_dir, q{internal});
    $self->_mkdir($internal_dir);

    my $src_dir = File::Spec->catdir($path, q{src});
    $self->_mkdir($src_dir);

    $self->gen_gtest_files($path);
}

sub gen_tests_dir {
    my ($self) = @_;
    my $path = File::Spec->catdir(File::Spec->curdir,
        $self->get_project_name(), q{tests});
    $self->_mkdir($path);

    my $cmake_file = File::Spec->catfile($path, q{CMakeLists.txt});
    my $template = <<'EOF';
INCLUDE_DIRECTORIES(${%s_SOURCE_DIR}/include)
INCLUDE_DIRECTORIES(${%s_SOURCE_DIR}/gtest/include)

FILE(GLOB test_files ${%s_SOURCE_DIR}/tests/*Test.cpp)
FOREACH(filename ${test_files})
    GET_FILENAME_COMPONENT(testname ${filename} NAME_WE)
    ADD_EXECUTABLE(${testname} ${filename})
    TARGET_LINK_LIBRARIES(${testname} %s gtest gtest-main)
    ADD_TEST(${testname} "${%s_BINARY_DIR}/tests/${testname}")
ENDFOREACH()
EOF
    my $proj = $self->get_project_name();
    my $package = $self->get_package_name();
    my $file = sprintf($template, $proj, $proj, $proj, $package, $proj);
    $self->_mkfile($cmake_file, \$file);
}

sub gen_scripts_dir {
    my ($self) = @_;
    my $path = File::Spec->catdir(File::Spec->curdir,
        $self->get_project_name(), q{scripts});
    $self->_mkdir($path);
    $self->gen_genclass_script($path);
    $self->gen_build_script($path);
    $self->gen_clean_script($path);
    $self->gen_test_script($path);
    $self->gen_install_script($path);
    $self->gen_xcode_script($path);
}

sub gen_test_script {
    my ($self, $path) = @_;
    my $file = <<'END_OF_SCRIPT';
#!/usr/bin/env perl

eval 'exec /usr/bin/perl -w -S $0 ${1+"$@"}' if 0;

use strict;
use warnings;

use FindBin;
use File::Find;
use Getopt::Long;
use Pod::Usage;
use IO::Dir;

my $help    = 0;
my $verbose = 0;
my $reload  = 0;

my %opt = (
    'help|?'     => \$help,
    'verbose|v!' => \$verbose,
    'reload|r'   => \$reload,
);
GetOptions(%opt);

pod2usage(1) if $help;

sub do_build {
    # build cmake command
    my $command = "cmake .. -DCMAKE_BUILD_TYPE=Debug";

    # check build directory
    my $build_dir = "$FindBin::Bin/../build";
    unless (-e $build_dir) {
        mkdir($build_dir, 0755) or die $!;
    }
    chdir $build_dir or die $!;
    print $command, "\n";
    system($command);
    print "make\n";
    system("make");

    if ($verbose) {
        my @tests = ();
        my $tests_dir = IO::Dir->new('tests');
        while (defined(my $filename = $tests_dir->read)) {
            next if (-d $filename);
            next if ($filename =~ /\.cmake$/);
            next if ($filename eq 'Makefile');
            push(@tests, $filename);
        }
        for my $test ( @tests ) {
            my $test_command = sprintf("./tests/%s", $test);
            print $test_command, "\n";
            system(sprintf($test_command));
        }
    } else {
        print "make test\n";
        system("make test");
    }
}

&do_build();
exit unless $reload;

my %FILES = ();
&find_updated_files();

print "1. Waiting for source changes... (press Ctrl+C to interrupt)\n";
my $build_count = 1;

while (1) {
    sleep 1;
    if (&find_updated_files()) {
        print "found updated file\n";
        print "start to build\n";
        &do_build();
        $build_count++;
        print sprintf("%d. Waiting for source changes... (press Ctrl+C to interrupt)\n", $build_count);
    }
}

sub find_updated_files {
    my $found = 0;
    for my $type (qw(include src tests)) {
        my $dir = "$FindBin::Bin/../" . $type;
        File::Find::find({ no_chdir => 1, wanted => sub {
            my $filepath = $File::Find::name; 
            return unless -f $filepath;
            my $last_update = (stat($filepath))[9];

            unless (exists $FILES{$filepath}) {
                $found = 1;
                $FILES{$filepath} = $last_update;
            } else {
                if ($FILES{$filepath} != $last_update) {
                    $found = 1;
                    $FILES{$filepath} = $last_update;
                }
            }
        } }, $dir);
    }
    return $found;
}


1;
__END__
=head1 NAME

test - This program helps your building and testing workaround.

=head1 SYNOPSIS

    test [options]

Example:

    ./scripts/test --verbose
    ./scripts/test --reload
    ./scripts/test -v -r

Options:

    -verbose (v) if this flag is on, print verbose results of google-tests. If not, the test results would be compressed.
    -reload  (r) if this flag is on, automatically find updated files, and build them, repeatedly.

=head1 DESCRIPTION

This helps your building and testing workaround.

=head1 AUTHOR

Lyo Kato, E<lt>lyo.kato@gmail.comE<gt>

=head1 COPYRIGHT AND LICENSE

